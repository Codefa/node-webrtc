{
    "name": "force-py2",
    "version": "0.1.0",
    "description": "Adds a symlink that makes `python` resolve to `python2` in npm scripts",
    "bin": {
        "python": "python2"
    },
    "scripts": {
        "preinstall": "run-script-os",
        "preinstall:nix": "ln -fs $(which python2) ./python2",
        "preinstall:default": "echo"
    },
    "dependencies": {
        "run-script-os": "^1.1.6"
    },
    "files": []
}
